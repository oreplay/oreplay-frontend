{
  "name": "oreplay-frontend",
  "private": true,
  "version": "0.3.4",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "format": "npx prettier . --write",
    "format:check": "npx prettier . --check",
    "lint": "eslint . --ext ts,tsx,json --report-unused-disable-directives --max-warnings 0",
    "before-commit": "npm run format && npm run lint && npm run build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "vitest"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/x-data-grid": "^7.15.0",
    "@mui/x-date-pickers": "^7.27.0",
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@nivo/radar": "^0.99.0",
    "@nivo/scatterplot": "^0.99.0",
    "@sentry/react": "^9.38.0",
    "@sentry/vite-plugin": "^3.6.0",
    "@toolpad/core": "^0.12.0",
    "@types/luxon": "^3.4.2",
    "axios": "^1.7.9",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "luxon": "^3.4.4",
    "qr-code-styling": "^1.9.2",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-i18next": "^6.1.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "3.4.2",
